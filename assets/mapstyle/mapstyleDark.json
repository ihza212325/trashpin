{
  "version": 8,
  "name": "Dark Matter",
  "metadata": {
    "maputnik:renderer": "mbgljs"
  },
  "sources": {
    "carto": {
      "type": "vector",
      "url": "https://tiles.basemaps.cartocdn.com/vector/carto.streets/v1/tiles.json"
    }
  },
  "sprite": "https://basemaps.cartocdn.com/gl/positron-gl-style/sprite",
  "glyphs": "https://tiles.basemaps.cartocdn.com/fonts/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "Background",
      "type": "background",
      "paint": {
        "background-color": [
          "interpolate",
          ["exponential", 1],
          ["zoom"],
          5,
          "hsl(86, 39%, 7%)",
          14,
          "hsl(120, 0%, 7%)"
        ]
      }
    },
    {
      "id": "Scrub",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "maxzoom": 9,
      "paint": {
        "fill-color": "hsl(73, 36%, 15%)",
        "fill-opacity": 1
      },
      "filter": ["==", "class", "scrub"]
    },
    {
      "id": "Tree",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "minzoom": 0,
      "maxzoom": 9,
      "paint": {
        "fill-color": "hsl(103, 42%, 13%)",
        "fill-opacity": 1
      },
      "filter": ["==", "class", "tree"]
    },
    {
      "id": "Forest",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "minzoom": 0,
      "maxzoom": 9,
      "paint": {
        "fill-color": "hsl(90, 39%, 20%)",
        "fill-opacity": 1
      },
      "filter": ["in", "class", "forest", "wood"]
    },
    {
      "id": "Wood",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "minzoom": 9,
      "paint": {
        "fill-color": "hsl(110, 54%, 18%)"
      },
      "filter": ["==", "class", "wood"]
    },
    {
      "id": "Grass",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "paint": {
        "fill-color": "hsla(110, 54%, 18%, 0.5)"
      },
      "filter": ["==", "class", "grass"]
    },
    {
      "id": "Sand",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "minzoom": 9,
      "paint": {
        "fill-color": "hsl(53, 62%, 12%)"
      },
      "filter": ["==", "class", "sand"]
    },
    {
      "id": "Glacier",
      "type": "fill",
      "source": "carto",
      "source-layer": "landcover",
      "paint": {
        "fill-color": "hsl(347, 0%, 20%)",
        "fill-opacity": 1
      },
      "filter": ["==", "class", "ice"]
    },
    {
      "id": "Residential",
      "type": "fill",
      "source": "carto",
      "source-layer": "landuse",
      "maxzoom": 16,
      "paint": {
        "fill-color": [
          "interpolate",
          ["exponential", 1],
          ["zoom"],
          6,
          "hsla(0, 0.9%, 12%, 0.3)",
          16,
          "hsla(0, 0.9%, 15%, 0.3)"
        ]
      },
      "filter": ["in", "class", "residential", "suburb", "neighbourhood"]
    },
    {
      "id": "Industrial",
      "type": "fill",
      "source": "carto",
      "source-layer": "landuse",
      "minzoom": 10,
      "paint": {
        "fill-color": "hsl(0, 0.9%, 13%)",
        "fill-opacity": 0.5
      },
      "filter": ["in", "class", "industrial", "quarry"]
    },
    {
      "id": "Commercial",
      "type": "fill",
      "source": "carto",
      "source-layer": "landuse",
      "minzoom": 10,
      "paint": {
        "fill-color": "hsl(0, 0.9%, 13%)",
        "fill-opacity": 0.5
      },
      "filter": ["in", "class", "commercial", "retail"]
    },
    {
      "id": "Water",
      "type": "fill",
      "source": "carto",
      "source-layer": "water",
      "paint": {
        "fill-color": [
          "interpolate",
          ["linear"],
          ["zoom"],
          6,
          "hsl(206, 78%, 20%)",
          22,
          "hsl(206, 78%, 25%)"
        ]
      }
    },
    {
      "id": "River",
      "type": "line",
      "source": "carto",
      "source-layer": "waterway",
      "minzoom": 3,
      "paint": {
        "line-color": "hsl(206, 78%, 25%)",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          0,
          0.5,
          4,
          1,
          13,
          1.25,
          16,
          1.5,
          22,
          6
        ]
      },
      "filter": ["!=", "brunnel", "tunnel"]
    },
    {
      "id": "Road network",
      "type": "line",
      "source": "carto",
      "source-layer": "transportation",
      "minzoom": 5,
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "hsl(0, 0%, 30%)",
        "line-width": [
          "interpolate",
          ["linear", 1],
          ["zoom"],
          5,
          ["match", ["get", "class"], "motorway", 0.5, 0],
          7,
          ["match", ["get", "class"], ["motorway", "trunk"], 1, 0],
          9,
          ["match", ["get", "class"], ["motorway", "trunk"], 2, 0],
          14,
          [
            "match",
            ["get", "class"],
            "motorway",
            5,
            ["primary", "trunk", "secondary", "tertiary"],
            1,
            0.5
          ],
          16,
          [
            "match",
            ["get", "class"],
            "motorway",
            10,
            ["primary", "trunk", "secondary", "tertiary"],
            4.6,
            3
          ],
          18,
          [
            "match",
            ["get", "class"],
            "motorway",
            12,
            ["primary", "trunk", "secondary", "tertiary"],
            5,
            7
          ],
          20,
          [
            "match",
            ["get", "class"],
            "motorway",
            15,
            ["primary", "trunk", "secondary", "tertiary"],
            6.4,
            8
          ],
          22,
          [
            "match",
            ["get", "class"],
            "motorway",
            16,
            ["primary", "trunk", "secondary", "tertiary"],
            12,
            ["minor", "service", "pier", "raceway"],
            11,
            9
          ]
        ]
      },
      "filter": [
        "all",
        ["!=", "brunnel", "tunnel"],
        ["!in", "class", "path", "track", "ferry"]
      ]
    },
    {
      "id": "Country border",
      "type": "line",
      "source": "carto",
      "source-layer": "boundary",
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "hsl(120, 2%, 50%)",
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 4, 0.6, 6, 1],
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          0,
          0.4,
          3,
          0.9,
          5,
          1.3,
          12,
          3
        ]
      },
      "filter": ["all", ["==", "admin_level", 2], ["==", "maritime", 0]]
    },
    {
      "id": "District and Village labels",
      "type": "symbol",
      "source": "carto",
      "source-layer": "place",
      "minzoom": 10,
      "maxzoom": 16,
      "layout": {
        "icon-image": "circle-dot",
        "icon-size": ["interpolate", ["linear", 1], ["zoom"], 10, 0.5, 14, 0.8],
        "text-anchor": "bottom",
        "text-field": "{name}",
        "text-font": ["Roboto Medium", "Noto Sans Regular"],
        "text-max-width": 8,
        "text-size": [
          "interpolate",
          ["linear", 1],
          ["zoom"],
          10,
          [
            "case",
            ["==", ["get", "class"], "district"],
            14,
            ["==", ["get", "class"], "village"],
            10,
            8
          ],
          14,
          [
            "case",
            ["==", ["get", "class"], "district"],
            18,
            ["==", ["get", "class"], "village"],
            14,
            12
          ]
        ]
      },
      "paint": {
        "text-color": "hsl(240, 6%, 87%)",
        "text-halo-color": "hsla(0, 0%, 0%, 0.8)",
        "text-halo-width": 1.6
      },
      "filter": ["in", "class", "district", "village"]
    },
    {
      "id": "City labels",
      "type": "symbol",
      "source": "carto",
      "source-layer": "place",
      "minzoom": 5,
      "maxzoom": 16,
      "layout": {
        "icon-image": "circle-dot",
        "icon-size": [
          "interpolate",
          ["linear", 1],
          ["zoom"],
          6,
          ["match", ["get", "capital"], 2, 0.7, 0.6],
          14,
          ["match", ["get", "capital"], 2, 0.9, 0.7]
        ],
        "text-anchor": "bottom",
        "text-field": "{name}",
        "text-font": ["Roboto Medium", "Noto Sans Regular"],
        "text-max-width": 8,
        "text-size": [
          "interpolate",
          ["linear", 1],
          ["zoom"],
          5,
          ["case", ["==", ["get", "capital"], 2], 16, 12],
          7,
          ["case", ["==", ["get", "capital"], 2], 18, 14],
          9,
          ["case", ["==", ["get", "capital"], 2], 25, 22],
          11,
          ["case", ["==", ["get", "capital"], 2], 28, 24]
        ]
      },
      "paint": {
        "text-color": "hsl(240, 6%, 87%)",
        "text-halo-color": "hsla(0, 0%, 0%, 0.8)",
        "text-halo-width": 1.6
      },
      "filter": ["==", "class", "city"]
    },
    {
      "id": "Country labels",
      "type": "symbol",
      "source": "carto",
      "source-layer": "place",
      "minzoom": 2,
      "maxzoom": 10,
      "layout": {
        "text-field": "{name}",
        "text-font": ["Roboto Bold", "Noto Sans Bold"],
        "text-max-width": ["case", ["<=", ["get", "rank"], 2], 8.25, 6.25],
        "text-size": [
          "interpolate",
          ["linear", 1],
          ["zoom"],
          2,
          ["case", ["<=", ["get", "rank"], 2], 10, 8],
          3,
          [
            "case",
            ["<=", ["get", "rank"], 2],
            12,
            [">=", ["get", "rank"], 3],
            10,
            9
          ],
          4,
          [
            "case",
            ["<=", ["get", "rank"], 2],
            16,
            [">=", ["get", "rank"], 3],
            14,
            12
          ],
          7,
          [
            "case",
            ["<=", ["get", "rank"], 2],
            22,
            [">=", ["get", "rank"], 3],
            18,
            16
          ]
        ]
      },
      "paint": {
        "text-color": "hsl(0, 3%, 75%)",
        "text-halo-color": "hsla(0, 0%, 0%, 0.8)",
        "text-halo-width": ["case", ["<=", ["get", "rank"], 2], 1.4, 1]
      },
      "filter": ["==", "class", "country"]
    },
    {
      "id": "Road labels",
      "type": "symbol",
      "source": "carto",
      "source-layer": "transportation_name",
      "minzoom": 12,
      "layout": {
        "symbol-placement": "line",
        "text-field": "{name}",
        "text-font": ["Roboto Regular", "Noto Sans Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12,
          8,
          14,
          10,
          16,
          12,
          20,
          16
        ],
        "text-max-angle": 30,
        "text-letter-spacing": 0.1,
        "text-padding": 1
      },
      "paint": {
        "text-color": "hsl(0, 0%, 100%)",
        "text-halo-color": "hsl(0, 0%, 0%)",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "park-areas",
      "type": "fill",
      "source": "carto",
      "source-layer": "landuse",
      "filter": ["==", "class", "park"],
      "paint": {
        "fill-color": "rgba(0, 100, 0, 0.2)",
        "fill-outline-color": "rgba(0, 100, 0, 0.7)"
      }
    },
    {
      "id": "neighborhood-labels",
      "type": "symbol",
      "source": "carto",
      "source-layer": "place",
      "minzoom": 12,
      "filter": [
        "all",
        ["in", "class", "suburb", "neighbourhood", "quarter"],
        ["!=", "name", ""]
      ],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Semibold"],
        "text-size": {
          "stops": [
            [12, 10],
            [16, 14]
          ]
        },
        "text-max-width": 8
      },
      "paint": {
        "text-color": "rgba(255, 255, 255, 0.7)",
        "text-halo-color": "rgba(0, 0, 0, 0.8)",
        "text-halo-width": 1
      }
    }
  ],
  "id": "dark-matter",
  "owner": "Carto"
}
